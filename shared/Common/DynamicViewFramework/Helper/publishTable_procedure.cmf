      <element name="scriptPath" direction="IN">
        <datatype type="STRING" maxLength="32768"/>
      </element>
      <element name="sqlScript" direction="IN">
        <datatype type="STRING" maxLength="32768"/>
      </element>
    </datatype>
  </dependency>
  <attribute name="Script" type="STRING">/* publishTable:
    For a newly introspected table, this procedure creates derived views in the layers and publishes the view created in Application layer.
 
	Input:
   	 	name: 					View name corresponding to an introspected table
   	 	sourcePath: 			The path to the introspected table
		formatLayerPath:		Path to formatting layer folder
		businessLayerPath:		Path to business layer folder
		appLayerPath:			Path to application layer folder
		publishPath:			Fully qualified path to the schema in publish area

	Output:
		N/A

	Author:      Venky Madireddi
	Date:        11/05/2014
	CIS Version: 6.2.6
*/
PROCEDURE publishTable
(
	IN	name					VARCHAR(255),
	IN	sourcePath				/lib/resource/ResourceDefs.ResourcePath,
	IN	formatLayerPath			/lib/resource/ResourceDefs.ResourcePath,
	IN	businessLayerPath		/lib/resource/ResourceDefs.ResourcePath,
	IN	appLayerPath			/lib/resource/ResourceDefs.ResourcePath,
	IN	publishPath				/lib/resource/ResourceDefs.ResourcePath,
	IN	inDebug					TINYINT
)
BEGIN
	DECLARE sqlScript, tmpPath VARCHAR(65535);
	DECLARE createLinkResp XML;
	DECLARE fault XML;

	CALL /shared/Common/DynamicViewFramework/Helper/logDebugMsg(&apos;Entered publishTable: &apos; || name, inDebug);

	-- Create View in Formatting Layer
	CALL /shared/Common/DynamicViewFramework/Helper/logDebugMsg(&apos;Creating view in formatting layer: &apos; || name, inDebug);
	CALL /shared/Common/DynamicViewFramework/Helper/createResource(formatLayerPath, name, &apos;TABLE&apos;, &apos;SQL_TABLE&apos;);
	CALL /shared/ASAssets/Utilities/repository/RepoUtils/applyReservedListToPath(sourcePath, NULL, tmpPath);
	SET sqlScript= &apos;select * from &apos; || tmpPath;
	CALL /shared/Common/DynamicViewFramework/Helper/updateSqlTable(formatLayerPath||&apos;/&apos;||name,sqlScript);
	CALL /shared/Common/DynamicViewFramework/Helper/copyResourcePrivileges(formatLayerPath, name, &apos;TABLE&apos;, 0, inDebug);

	-- Create View in Business Layer
	CALL /shared/Common/DynamicViewFramework/Helper/logDebugMsg(&apos;Creating view in business layer: &apos; || name, inDebug);
	CALL /shared/Common/DynamicViewFramework/Helper/createResource(businessLayerPath, name, &apos;TABLE&apos;, &apos;SQL_TABLE&apos;);
	CALL /shared/ASAssets/Utilities/repository/RepoUtils/applyReservedListToPath(formatLayerPath || &apos;/&apos; || name, NULL, tmpPath);
	SET sqlScript= &apos;select * from &apos; || tmpPath;
	CALL /shared/Common/DynamicViewFramework/Helper/updateSqlTable(businessLayerPath||&apos;/&apos;||name,sqlScript);
	CALL /shared/Common/DynamicViewFramework/Helper/copyResourcePrivileges(businessLayerPath, name, &apos;TABLE&apos;, 0, inDebug);

	-- Create View in Application Layer
	CALL /shared/Common/DynamicViewFramework/Helper/logDebugMsg(&apos;Creating view in application layer: &apos; || name, inDebug);
	CALL /shared/Common/DynamicViewFramework/Helper/createResource(appLayerPath, name, &apos;TABLE&apos;, &apos;SQL_TABLE&apos;);
	CALL /shared/ASAssets/Utilities/repository/RepoUtils/applyReservedListToPath(businessLayerPath || &apos;/&apos; || name, NULL, tmpPath);
	SET sqlScript= &apos;select * from &apos; || tmpPath;
	CALL /shared/Common/DynamicViewFramework/Helper/updateSqlTable(appLayerPath||&apos;/&apos;||name,sqlScript);
	CALL /shared/Common/DynamicViewFramework/Helper/copyResourcePrivileges(appLayerPath, name, &apos;TABLE&apos;, 0, inDebug);

	CALL /shared/Common/DynamicViewFramework/Helper/logDebugMsg(&apos;Publishing view: &apos; || name, inDebug);
	CALL /services/webservices/system/admin/resource/operations/createLink(&apos;
			&lt;resource:createLink xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
			  &lt;resource:path&gt;&apos; || publishPath || &apos;&lt;/resource:path&gt;
			  &lt;resource:name&gt;&apos; || name || &apos;&lt;/resource:name&gt;
			  &lt;resource:detail&gt;SIMPLE&lt;/resource:detail&gt;
			  &lt;resource:targetPath&gt;&apos; || appLayerPath || &apos;/&apos; || name || &apos;&lt;/resource:targetPath&gt;
			  &lt;resource:targetType&gt;TABLE&lt;/resource:targetType&gt;
			  &lt;resource:annotation&gt;&lt;/resource:annotation&gt;
			&lt;/resource:createLink&gt;
			&apos;, createLinkResp, fault);
	CALL /shared/Common/DynamicViewFramework/Helper/copyResourcePrivileges(publishPath, name, &apos;LINK&apos;, 0, inDebug);

	CALL /shared/Common/DynamicViewFramework/Helper/logDebugMsg(&apos;Exiting publishTable: &apos; || name, inDebug);

END</attribute>
