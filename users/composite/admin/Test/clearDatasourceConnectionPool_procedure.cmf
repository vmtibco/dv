  <attribute name="Script" type="STRING">/*
    The script clears connection pool for a data source.

	Input:
        dsPath - TDV source folder path to the data source in
            Values: e.g. /shared/examples/ds_inventory

    Output:
        status - SUCCESS or FAIL
        message - Detailed message
*/
PROCEDURE clearDatasourceConnectionPool
(
	IN	dsPath		/lib/resource/ResourceDefs.ResourcePath,
	OUT	status		VARCHAR,
	OUT	message		VARCHAR(4096)
)
BEGIN
	DECLARE reqXML, respXML, faultXML	XML;
	SET reqXML = &apos;
&lt;ns1:performServerAction xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/server&quot; xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
 &lt;ns1:actionName&gt;ClearDataSourceConnectionPools&lt;/ns1:actionName&gt;
 &lt;ns1:attributes&gt;
  &lt;ns2:attribute xmlns:ns2=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
   &lt;ns2:name&gt;dataSourcePaths&lt;/ns2:name&gt;
   &lt;ns2:type&gt;STRING_ARRAY&lt;/ns2:type&gt;
   &lt;ns2:valueArray&gt;
    &lt;ns2:item&gt;&apos; || dsPath || &apos;&lt;/ns2:item&gt;
   &lt;/ns2:valueArray&gt;
  &lt;/ns2:attribute&gt;
 &lt;/ns1:attributes&gt;
&lt;/ns1:performServerAction&gt;
&apos;;
	CALL /services/webservices/system/admin/server/operations/performServerAction(reqXML, respXML, faultXML);
    IF (faultXML IS NOT NULL) THEN
		SET status = &apos;FAIL&apos;;
	ELSE
		SET status = CAST(XPATH (respXML, &apos;//server:status/text()&apos;) AS VARCHAR);
		SET message = CAST(XPATH (respXML, &apos;//common:message/text()&apos;) AS VARCHAR(4096));
    END IF;
EXCEPTION
	ELSE
		SET status = &apos;EXCEPTION&apos;;
		SET message = CURRENT_EXCEPTION.MESSAGE;
END</attribute>
